{
  "name": "brick-sequelize",
  "version": "0.0.0",
  "description": "sequelize plugin for brick engine",
  "main": "index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "docs": "jsdoc -c conf.json",
    "build": "tsc -d",
    "build:w": "tsc -d -w",
    "lint": "eslint --fix .",
    "check": "npm run build && npm run lint",
    "test": "npm run check && jest --coverage --env=node --runInBand --detectOpenHandles",
    "watch": "jest --watch"
  },
  "files": [
    "typings",
    "package.json",
    "lib",
    "app.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiba-zhao/brick-sequelize.git"
  },
  "bugs": {
    "url": "https://github.com/kiba-zhao/brick-sequelize/issues"
  },
  "homepage": "https://github.com/kiba-zhao/brick-sequelize#readme",
  "keywords": [
    "sequelize",
    "brick"
  ],
  "author": "kiba.x.zhao",
  "license": "MIT",
  "devDependencies": {
    "@types/faker": "^5.1.7",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@types/sequelize": "^4.28.9",
    "brick-engine": "file:../brick-engine",
    "eslint": "^7.17.0",
    "eslint-config-egg": "^9.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "faker": "^5.4.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "mysql2": "^2.2.5",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "brick-engine": "file:../brick-engine"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "sequelize": "^6.5.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "lib/**/*.js",
      "app.js",
      "index.js"
    ]
  }
}
